#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch;
    char word[20],sen[100];
    
    scanf("%c", &ch);
    scanf("%s", word);
    scanf("\n");  //也可以放空格
    scanf("%[^\n]%*c",sen);
    
    
    printf("%c\n", ch);
    printf("%s\n", word);
    printf("%s\n",sen);
       
    return 0;
}
